package pbankaccounts;

public class BankAccounts {

	private int balance;
	
	public BankAccounts() {
		balance = 0;
	}
	
	public int getBalance() {
		return balance;
	}
	
	public void deposit(int i) {
		balance += i;
	}
	
	public void withdraw(int i) {
		balance -= i;
		if (balance < 0) {
			balance -= 5;
		}
	}
}
