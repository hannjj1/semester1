package probot;

public class Robot {

	private String name;
	private String fuel;
	
	public Robot(String rName, String rFuel) {
		name = rName;
		fuel = rFuel;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName() {
		this.name = name;
	}
	
	public String getFuel() {
		return fuel;
	}
	
	public void setFuel() {
		this.fuel = fuel;
	}
	
//	public static void main(String[] args) {
//
//	}

}
